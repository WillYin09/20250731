{
  "envId": "cloud1-8gxj5xyl2938bec3",
  "functionRoot": "./",
  "functions": [
    {
      "name": "tarot-app",
      "runtime": "Nodejs16.13",
      "memorySize": 512,
      "timeout": 30,
      "triggers": [
        {
          "name": "httpTrigger",
          "type": "http",
          "config": {
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
          }
        }
      ],
      "installDependency": true,
      "ignore": [
        "node_modules",
        ".git",
        ".next",
        "README.md",
        ".env.local",
        "env.example"
      ]
    }
  ],
  "framework": {
    "name": "next",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-next",
        "inputs": {
          "entry": "./",
          "buildCommand": "npm run build",
          "outputPath": ".next"
        }
      }
    }
  }
} 